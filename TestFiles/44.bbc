using System.Math;
using System;

struct Vector2
{
	int x;
	int y;
}

Vector2 normalize(this Vector2 vec)
{
	int a = vec.x * vec.x;
	int b = vec.y * vec.y;
	int l = sqrt(a + b);

	Vector2 res;
	res.x = vec.x / l;
	res.y = vec.y / l;
	return res;
}

Vector2 vec;
vec.x = 0;
vec.y = 6;

vec = vec.normalize();

printline(vec.x);
printline(vec.y);

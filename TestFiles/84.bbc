using System.Console;
using System.Vectors;
using System.Math;

void printline(float2 v)
{
    print('(');
    print(v.x);
    print(',');
    print(v.y);
    print(')');
    printline();
}

float2 v1 = new float2(8f, 0f);
float2 v2 = new float2(1f, 0f);

printline(v1);
printline(v2);

printline(distance(v1, v2));

v2.y = 0f;

printline(distance(v1, v2));

v1 = normalise(v1);
v2 = normalise(v2);

printline(dot(v1, v2));

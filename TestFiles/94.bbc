using System;
using System.Console;
using System.Tasks;

void main(Task* self)
{
    PrintLine("hello");
    Schedule(task, task_done);
    Schedule(task, task_done);
}

void task(Task* self)
{
    PrintLine("task");
}

void task_done(Task* self)
{
    PrintLine("task done");
}

Schedule(main);

Run();

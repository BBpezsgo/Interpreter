using System.Memory;

class Player {
	int x;
}

struct Point {
	int x;
	int y;
}

Point NewPoint(int x, int y) {
	Point result = new Point;
	result.x = x;
	result.y = y;
	return result;
}

void TestPoint() {
	Point p0 = NewPoint(55, 48);
	Point p1 = new Point;

	p1.x = 5;
	p1.y = 4;

	PrintLine(p0.x);

	PrintLine(p0.y);

	p0 = p1;

	PrintLine(p0.x);

	PrintLine(p0.y);
}

